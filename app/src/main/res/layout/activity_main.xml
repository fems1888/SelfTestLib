<?xml version="1.0" encoding="utf-8"?>
<com.example.jackieyao.selftestlib.AdCountDownLayout2 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jackieyao.selftestlib.MainActivity">


    <com.example.jackieyao.selftestlib.AdCountDownView2
        android:id="@+id/countDownView"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/countDownView"
        android:layout_centerHorizontal="true"
        android:text="启动"
        android:id="@+id/button"/>

</com.example.jackieyao.selftestlib.AdCountDownLayout2>
